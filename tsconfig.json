{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "alwaysStrict": true,
    "removeComments": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "declaration": true,
    "outDir": "./lib",
    "lib": [
      "es2015"
    ]
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
